{
  "name": "books-api",
  "version": "1.0.0",
  "description": "Books api",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "npm run lint:code",
    "lint:code": "eslint \"./src/**/*.ts\"",
    "lint:code:fix": "eslint \"./src/**/*.ts\" --fix",
    "test": "jest --coverage",
    "test:unit": "jest --coverage --testPathIgnorePatterns .integration.spec",
    "test:integration": "jest --testPathPattern .integration.spec",
    "build": "./node_modules/typescript/bin/tsc"
  },
  "dependencies": {
    "aws-sdk": "^2.749.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.10",
    "@types/uuid": "^8.3.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.1.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7"
  },
  "license": "ISC"
}
